[run]
omit =
    */__init__.py
    teafacto/scripts/*
    teafacto/oldscripts/*
    teafacto/eval/*
    teafacto/models/*
    teafacto/datahelp/*
    test
branch = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    */__init__.py
    teafacto/scripts/*
    teafacto/oldscripts/*
    teafacto/eval/*
    teafacto/models/*
    teafacto/datahelp/*
    test